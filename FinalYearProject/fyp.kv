    name: "minigame"
    canvas:
        Color:
            rgba: 1, 1, 1, 1
        Line:
            width: 2
            points: (0, self.height*0.9, self.width, self.height*0.9)
        Line:
            width: 2
            points: (self.width*0.4, self.height*0.45, self.width*0.6, self.height*0.45)
