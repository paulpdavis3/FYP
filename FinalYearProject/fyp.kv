<TitlePage>:
    name: "title"
    Image:
        size_hint: 0.6, 0.6
	    pos_hint: {"center_x": 0.5, 'center_y':0.675}
        source: "logo.png"
        y: self.parent.y
        x: self.parent.x
        size: self.parent.size
    Button:
        size_hint: 0.6, 0.2
	    pos_hint: {"center_x": 0.5, 'center_y':0.3}
	    background_normal: "button.png"
	    background_down: "buttondown.png"
        text: "Register"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)
        on_release: app.root.current = "register"

    Button:
	    pos_hint: {"center_x": 0.5, 'center_y':0.1}
	    size_hint: 0.6, 0.2
        text: "Login"
        on_release: app.root.current = "login"
	    background_normal: "button.png"
	    background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)

<LoginPage>:
    name: "login"
    Label:
        text: "Login Page"
        pos_hint: {"center_x": 0.5, 'center_y':0.9}
        size_hint: 0.1, 0.1
        bold: True
        font_size: self.width
    Label:
        text: 'Username'
        pos_hint: {"center_x": 0.5, 'center_y':0.775}
        font_size: "20dp"
        size_hint: 0.5, 0.1
        bold: True
    TextInput:
        id: usernameLogin
        text: root.username
        pos_hint: {"center_x": 0.5, 'center_y':0.7}
        size_hint: 0.5, 0.1
        font_size: self.height * 0.5
    Label:
        text: 'Password'
        pos_hint: {"center_x": 0.5, 'center_y':0.575}
        font_size: "20dp"
        size_hint: 0.5, 0.1
        bold: True
    TextInput:
        id: passwordLogin
        text: root.password
        password: True
        allow_copy: False
        pos_hint: {"center_x": 0.5, 'center_y':0.5}
        size_hint: 0.5, 0.1
        font_size: self.height * 0.8
    Button:
        text: "Login"
        on_release: root.checkLogin(usernameLogin.text, passwordLogin.text)
        pos_hint: {"center_x": 0.5, 'center_y':0.325}
        size_hint: 0.6, 0.2
        background_normal: "button.png"
        background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)
    Label:
        text: "Don't have an account? Click below"
        pos_hint: {"center_x": 0.5, 'center_y':0.2}
        font_size: "20dp"
        size_hint: 0.5, 0.1
        bold: True
    Button:
        text: "Register"
        on_release: app.root.current = "register"
        pos_hint: {"center_x": 0.5, 'center_y':0.1}
        size_hint: 0.6, 0.2
        background_normal: "button.png"
        background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)


<RegisterPage>:
    name: "register"
    Label:
        text: "Register Page"
        pos_hint: {"center_x": 0.5, 'center_y':0.9}
        size_hint: 0.1, 0.1
        bold: True
        font_size: self.width
    Label:
        text: 'Username'
        pos_hint: {"center_x": 0.5, 'center_y':0.775}
        font_size: "20dp"
        size_hint: 0.5, 0.075
        bold: True
    TextInput:
        id: usernameRegister
        pos_hint: {"center_x": 0.5, 'center_y':0.7}
        size_hint: 0.5, 0.075
        font_size: self.height * 0.5
    Label:
        text: "Email"
        pos_hint: {"center_x": 0.5, 'center_y':0.625}
        font_size: "20dp"
        size_hint: 0.5, 0.075
        bold: True
    TextInput:
        id: emailRegister
        pos_hint: {"center_x": 0.5, 'center_y':0.55}
        size_hint: 0.5, 0.075
        font_size: self.height * 0.5
    Label:
        text: 'Password'
        pos_hint: {"center_x": 0.5, 'center_y':0.475}
        font_size: "20dp"
        size_hint: 0.5, 0.075
        bold: True
    TextInput:
        id: passwordRegister
        password: True
        allow_copy: False
        pos_hint: {"center_x": 0.5, 'center_y':0.4}
        size_hint: 0.5, 0.075
        font_size: self.height * 0.5
    CheckBox:
        id: teacherCheckBox
        color: 1, 1, 1, 1
        pos_hint: {"center_x": 0.3, 'center_y':0.3125}
        size_hint: 0.05, 0.05
    Label:
        text: "Check box if you are a teacher"
        pos_hint: {"center_x": 0.5, 'center_y':0.3125}
        bold: True
        font_size: "20dp"
    Button:
        text: "Register"
        on_release: root.checkPassword(usernameRegister.text, emailRegister.text, passwordRegister.text, teacherCheckBox.active)
        pos_hint: {"center_x": 0.5, 'center_y':0.225}
        size_hint: 0.5, 0.15
        background_normal: "button.png"
        background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)
    Label:
        text: "Already have an account? Click below"
        pos_hint: {"center_x": 0.5, 'center_y':0.15}
        font_size: "20dp"
        size_hint: 0.5, 0.075
        bold: True

    Button:
        text: "Login"
        on_release: app.root.current = "login"
        pos_hint: {"center_x": 0.5, 'center_y':0.075}
        size_hint: 0.5, 0.15
        background_normal: "button.png"
        background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)


