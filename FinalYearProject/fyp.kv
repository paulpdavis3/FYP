ScreenManagement:
    TitlePage:
    MainPage:
    LoginPage:
    RegisterPage:
    StudentClassroomPage:
    TeacherClassroomPage:
    ProfilePage:
    ProgressPage:
    PlayPage:
    AdditionPage:
    SubtractionPage:
    MultiplicationPage:
    DivisionPage:

<TitlePage>:
    name: "title"
    Image:
        size_hint: 0.6, 0.6
	    pos_hint: {"center_x": 0.5, 'center_y':0.675}
        source: "logo.png"
        y: self.parent.y
        x: self.parent.x
        size: self.parent.size
    Button:
        size_hint: 0.6, 0.2
	    pos_hint: {"center_x": 0.5, 'center_y':0.3}
	    background_normal: "button.png"
	    background_down: "buttondown.png"
        text: "Register"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)
        on_release: app.root.current = "register"

    Button:
	    pos_hint: {"center_x": 0.5, 'center_y':0.1}
	    size_hint: 0.6, 0.2
        text: "Login"
        on_release: app.root.current = "login"
	    background_normal: "button.png"
	    background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)

<LoginPage>:
    name: "login"
    Label:
        text: "Login Page"
        pos_hint: {"center_x": 0.5, 'center_y':0.9}
        size_hint: 0.1, 0.1
        bold: True
        font_size: self.width
    Label:
        text: 'Username'
        pos_hint: {"center_x": 0.5, 'center_y':0.775}
        font_size: "20dp"
        size_hint: 0.5, 0.1
        bold: True
    TextInput:
        id: usernameLogin
        text: root.username
        pos_hint: {"center_x": 0.5, 'center_y':0.7}
        size_hint: 0.5, 0.1
    Label:
        text: 'Password'
        pos_hint: {"center_x": 0.5, 'center_y':0.575}
        font_size: "20dp"
        size_hint: 0.5, 0.1
        bold: True
    TextInput:
        id: passwordLogin
        text: root.password
        password: True
        allow_copy: False
        pos_hint: {"center_x": 0.5, 'center_y':0.5}
        size_hint: 0.5, 0.1
    Button:
        text: "Login"
        on_release: root.checkLogin(usernameLogin.text, passwordLogin.text)
        pos_hint: {"center_x": 0.5, 'center_y':0.325}
        size_hint: 0.6, 0.2
        background_normal: "button.png"
        background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)
    Label:
        text: "Don't have an account? Click below"
        pos_hint: {"center_x": 0.5, 'center_y':0.2}
        font_size: "20dp"
        size_hint: 0.5, 0.1
        bold: True
    Button:
        text: "Register"
        on_release: app.root.current = "register"
        pos_hint: {"center_x": 0.5, 'center_y':0.1}
        size_hint: 0.6, 0.2
        background_normal: "button.png"
        background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)


<RegisterPage>:
    name: "register"
    Label:
        text: "Register Page"
        pos_hint: {"center_x": 0.5, 'center_y':0.9}
        size_hint: 0.1, 0.1
        bold: True
        font_size: self.width
    Label:
        text: 'Username'
        pos_hint: {"center_x": 0.5, 'center_y':0.75}
        font_size: "20dp"
        size_hint: 0.5, 0.075
        bold: True
    TextInput:
        id: usernameRegister
        pos_hint: {"center_x": 0.5, 'center_y':0.675}
        size_hint: 0.5, 0.075
    Label:
        text: "Parent's Email"
        pos_hint: {"center_x": 0.5, 'center_y':0.6}
        font_size: "20dp"
        size_hint: 0.5, 0.075
        bold: True
    TextInput:
        id: emailRegister
        pos_hint: {"center_x": 0.5, 'center_y':0.525}
        size_hint: 0.5, 0.075
    Label:
        text: 'Password'
        pos_hint: {"center_x": 0.5, 'center_y':0.45}
        font_size: "20dp"
        size_hint: 0.5, 0.075
        bold: True
    TextInput:
        id: passwordRegister
        password: True
        allow_copy: False
        pos_hint: {"center_x": 0.5, 'center_y':0.375}
        size_hint: 0.5, 0.075
    Button:
        text: "Register"
        on_release: root.checkPassword(usernameRegister.text, emailRegister.text, passwordRegister.text)
        pos_hint: {"center_x": 0.5, 'center_y':0.225}
        size_hint: 0.5, 0.15
        background_normal: "button.png"
        background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)
    Label:
        text: "Already have an account? Click below"
        pos_hint: {"center_x": 0.5, 'center_y':0.15}
        font_size: "20dp"
        size_hint: 0.5, 0.075
        bold: True

    Button:
        text: "Login"
        on_release: app.root.current = "login"
        pos_hint: {"center_x": 0.5, 'center_y':0.075}
        size_hint: 0.5, 0.15
        background_normal: "button.png"
        background_down: "buttondown.png"
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)


<MainPage>:
    name: 'main'
    GridLayout:
        cols: 1
        Label:
            text: 'Home Page'
        GridLayout:
            cols: 1
            Button:
                id: playButton
                text: "Play"
                on_release: app.root.current = "play"
            Button:
                id: classroomButton
                text: 'Classroom'
                on_release: root.goToClassroom()
            Button:
                id: viewProfileButton
                text: "View Profile"
                on_release: app.root.current = "profile"
            Button:
                id: ViewProgressButton
                text: "View Progress"
                on_release: app.root.current = "progress"

<ProgressPage>:
    name: "progress"
    Button:
		on_release: app.root.current = "main"
		size_hint: 0.1, 0.1
	    pos_hint: {"left": 1, 'top':1}
        Image:
	        source: "back.png"
	        y: self.parent.y
            x: self.parent.x
            size: self.parent.size
	Label:
	    text: "This is where the progress page will be"

<ProfilePage>:
    name: "profile"
    Button:
		on_release: app.root.current = "main"
		size_hint: 0.1, 0.1
	    pos_hint: {"left": 1, 'top':1}
        Image:
	        source: "back.png"
	        y: self.parent.y
            x: self.parent.x
            size: self.parent.size
	Label:
	    text: "This is where the profile will be"


<PlayPage>:
    name: "play"
    Button:
		on_release: app.root.current = "main"
		size_hint: 0.1, 0.1
	    pos_hint: {"left": 1, 'top':1}
        Image:
	        source: "back.png"
	        y: self.parent.y
            x: self.parent.x
            size: self.parent.size

    Button:
        size_hint: 0.75, 0.2
        pos_hint: {'center_x': 0.5, 'center_y': 0.825}
        on_release: app.root.current = "addition"
        text: "Addition"
    Button:
        size_hint: 0.75, 0.2
        pos_hint: {'center_x': 0.5, 'center_y': 0.6}
        on_release: app.root.current = "subtraction"
        text: "Subtraction"
    Button:
        size_hint: 0.75, 0.2
        pos_hint: {'center_x': 0.5, 'center_y': 0.375}
        on_release: app.root.current = "multiplication"
        text: "Multiplication"
    Button:
        size_hint: 0.75, 0.2
        pos_hint: {'center_x': 0.5, 'center_y': 0.15}
        on_release: app.root.current = "division"
        text: "Division"

<StudentClassroomPage>:
    name: "studentclassroom"
    Button:
		on_release: app.root.current = "main"
		size_hint: 0.1, 0.1
	    pos_hint: {"left": 1, 'top':1}
        Image:
	        source: "back.png"
	        y: self.parent.y
            x: self.parent.x
            size: self.parent.size

    GridLayout:
        cols: 1
        Label
            size_hint: 0.8, 0.2
            font_size: root.width/10
            text: "Student"
        Label
            size_hint: 0.8, 0.2
            text: root.classroom
        TextInput:
            id: classroomInput
            font_size: classroomInput.height / 1.2
            size_hint: 0.8, 0.2
            disabled: False if root.classroom == "no" else True
        Button:
            size_hint: 0.8, 0.2
            text: "Join Classroom"
            on_release: root.joinClassroom(classroomInput.text)
            disabled: False if root.classroom == "no" else True
        Button:
            size_hint: 0.8, 0.2
            text: "Delete"
            on_release: root.deleteClassroom()
            disabled: True if root.classroom == "no" else False


<TeacherClassroomPage>:
    name: "teacherclassroom"
    Button:
		on_release: app.root.current = "main"
		size_hint: 0.1, 0.1
	    pos_hint: {"left": 1, 'top':1}
        Image:
	        source: "back.png"
	        y: self.parent.y
            x: self.parent.x
            size: self.parent.size

    Label:
        pos_hint: {'center_x': 0.5, 'center_y': 0.9}
        size_hint: 0.2, 0.2
        font_size: root.width/10
        text: "Student"

    Label:
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
        size_hint: 0.2, 0.2
        font_size: root.width/10
        text: "student"

<AdditionPage>:
    name: "addition"
    Button:
		on_release: app.root.current = "play"
		size_hint: 0.1, 0.1
	    pos_hint: {"left": 1, 'top':1}
