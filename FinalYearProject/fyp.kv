<StudentClassroomPage>:
    name: "studentclassroom"
    Button:
		on_release: app.root.current = "main"
		size_hint: 0.1, 0.1
	    pos_hint: {"left": 1, 'top':1}
        Image:
	        source: "back.png"
	        y: self.parent.y
            x: self.parent.x
            size: self.parent.size

    Label:
        text: "Student's Classroom"
        pos_hint: {"center_x": 0.5, 'center_y':0.825}
        size_hint: 0.1, 0.1
        bold: True
        font_size: self.width
    Label
        pos_hint: {"center_x": 0.5, 'center_y':0.675}
        size_hint: 0.8, 0.2
        text: root.classroom
    TextInput:
        id: classroomInput
        pos_hint: {"center_x": 0.5, 'center_y':0.6}
        size_hint: 0.5, 0.1
        font_size: self.height * 0.5
        disabled: False if root.classroom == "no classroom" else True
