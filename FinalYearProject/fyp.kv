        text: "Join Classroom"
        on_release: root.joinClassroom(classroomInput.text)
        disabled: False if root.classroom == "no classroom" else True
        pos_hint: {"center_x": 0.5, 'center_y':0.4}
        size_hint: 0.7, 0.2
        background_normal: "button.png"
        background_down: "buttondown.png"
        background_disabled_normal: 'buttondisabled.png'
        font_size: "20dp"
        bold: True
        color: (1, 72/255, 72/255, 1)
    Button:
        text: "Delete"
        on_release: root.deleteClassroom()
        disabled: True if root.classroom == "no classroom" else False
        pos_hint: {"center_x": 0.5, 'center_y':0.15}
        size_hint: 0.7, 0.2
        background_normal: "button.png"
        background_down: "buttondown.png"
